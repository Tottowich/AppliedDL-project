journal = {European biophysics journal : EBJ},
keywords = {Chemical,Computer Simulation,Elasticity,Immunoglobulin G,Immunoglobulin G: chemistry,Mechanical,Models,Molecular,Optical Tweezers,Protein Binding,Staphylococcal Protein A,Staphylococcal Protein A: chemistry,Stress,antibodies},
