doi = {10.1038/sj.ki.5002230},
file = {:E$\backslash$:/Mina Dokument/Mendeley/Lane, Mobley - Role of P-fimbrial-mediated adherence in pyelonephritis and persistence of uropathogenic Escherichia coli (UPEC) in the m.pdf:pdf},
issn = {0085-2538},
journal = {Kidney international},
keywords = {Adhesins, Escherichia coli,Adhesins, Escherichia coli: analysis,Adhesins, Escherichia coli: physiology,Amino Acid Sequence,Animals,Escherichia coli,Escherichia coli Infections,Escherichia coli Infections: pathology,Escherichia coli Infections: physiopathology,Escherichia coli: pathogenicity,Fimbriae Proteins,Fimbriae Proteins: analysis,Fimbriae Proteins: physiology,Fimbriae, Bacterial,Fimbriae, Bacterial: physiology,Humans,Kidney,Kidney: microbiology,Kidney: pathology,Kidney: physiopathology,Molecular Sequence Data,Pyelonephritis,Pyelonephritis: physiopathology},
