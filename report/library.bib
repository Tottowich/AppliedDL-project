keywords = {chaperone,fimbrial adhesions,gram-negative pathogens,immunomodulatory,mono- and,polyadhesive binding,usher machinery},
month = {dec},
pmid = {20070375},
title = {{Adhesive organelles of Gram-negative pathogens assembled with the classical chaperone/usher machinery: structure and function from a clinical standpoint.}},
