number = {August},
title = {{Bioaerosol standoff detection and correlation assessment with concentration and viability point sensors}},
url = {http://pubs.drdc.gc.ca/PDFS/unc107/p534626{\_}A1b.pdf},
volume = {2008},
year = {2010}
}
@article{DeLucia2009,
abstract = {Recently laser-induced breakdown spectroscopy (LIBS) has been investigated as a potential technique for trace explosive detection. Typically LIBS is performed using nanosecond laser pulses. For this work, we have investigated the use of femtosecond laser pulses for explosive residue detection at two different fluences. Femtosecond laser pulses have previously been shown to provide several advantages for laser ablation and other LIBS applications. We have collected LIBS spectra of several bulk explosives and explosive residues at different pulse durations and energies. In contrast to previous femtosecond LIBS spectra of explosives, we have observed atomic emission peaks for the constituent elements of explosives - carbon, hydrogen, nitrogen, and oxygen. Preliminary results indicate that several advantages attributed to femtosecond pulses are not realized at higher laser fluences.},
author = {{De Lucia}, Frank C and Gottfried, Jennifer L and Miziolek, Andrzej W},
