pages = {1514--1523},
pmid = {22500751},
publisher = {Biophysical Society},
title = {{Direct upstream motility in Escherichia coli}},
url = {http://dx.doi.org/10.1016/j.bpj.2012.03.001},
volume = {102},
year = {2012}
}
