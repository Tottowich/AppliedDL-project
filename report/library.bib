journal = {European biophysics journal},
keywords = {Bacteria,Bacteria: metabolism,Bacterial,Bacterial Adhesion,Bacterial Adhesion: physiology,Bacterial Physiological Phenomena,Bacterial: physiology,Biological,Biomechanics,Biopolymers,Biopolymers: chemistry,Biopolymers: metabolism,Computer Simulation,Fimbriae,Mechanical,Models,Stress},
month = {jun},
