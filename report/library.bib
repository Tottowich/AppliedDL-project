doi = {10.1038/mi.2011.41},
file = {:E$\backslash$:/Mina Dokument/Mendeley/Mantis, Rol, Corth{\'{e}}sy - Secretory IgA's complex roles in immunity and mucosal homeostasis in the gut. - 2011.pdf:pdf},
