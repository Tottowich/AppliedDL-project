pages = {2714--26},
pmid = {15695629},
title = {{Coalescence of membrane tethers: experiments, theory, and applications.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1305367{\&}tool=pmcentrez{\&}rendertype=abstract},
