issn = {1878-7568},
journal = {Acta biomaterialia},
keywords = {Computer Simulation,Mechanics,Models, Chemical,Models, Molecular,Protein Conformation,Protein Denaturation,Protein Folding,Proteins,Proteins: chemistry,Proteins: ultrastructure,Stress, Mechanical},
month = {jul},
