doi = {10.7554/eLife.02403},
eprint = {1405.6489},
file = {:E$\backslash$:/Mina Dokument/Mendeley/Kantsler et al. - Rheotaxis facilitates upstream navigation of mammalian sperm cells. - 2014.pdf:pdf},
