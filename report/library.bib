journal = {Vaccine},
keywords = {Bacterial Toxins,Bacterial Toxins: biosynthesis,Enterotoxigenic Escherichia coli,Enterotoxigenic Escherichia coli: classification,Enterotoxigenic Escherichia coli: immunology,Enterotoxigenic Escherichia coli: isolation {\&} puri,Enterotoxins,Enterotoxins: biosynthesis,Escherichia coli Infections,Escherichia coli Infections: epidemiology,Escherichia coli Proteins,Escherichia coli Proteins: biosynthesis,Escherichia coli Vaccines,Fimbriae Proteins,Fimbriae Proteins: biosynthesis,Humans},
month = {aug},
