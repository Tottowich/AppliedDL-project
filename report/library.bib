doi = {10.1369/jhc.5A6794.2005},
file = {:E$\backslash$:/Mina Dokument/Mendeley/L{\"{u}}di et al. - Assessing the expression of enterotoxigenic Escherichia coli-specific surface antigens in recombinant strains by transmiss.pdf:pdf},
