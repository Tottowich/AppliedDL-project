series = {OSA Trends in Optics and Photonics Series},
title = {{Temporal and spatial evolution of a laser-induced plasma from a steel target.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/14658707},
volume = {57},
year = {2003}
}
