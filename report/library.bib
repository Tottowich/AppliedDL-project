doi = {10.1002/mnfr.200900535},
file = {:E$\backslash$:/Mina Dokument/Mendeley/Liu et al. - Direct adhesion force measurements between E. coli and human uroepithelial cells in cranberry juice cocktail. - 2010.pdf:pdf},
issn = {1613-4133},
journal = {Molecular nutrition {\&} food research},
keywords = {2009,2010,april 6,atomic force microscopy,bacterial adhesion,cranberry,november 6,received,revised,uroepithelial cell},
month = {jun},
pages = {1744--1752},
pmid = {20568234},
title = {{Direct adhesion force measurements between E. coli and human uroepithelial cells in cranberry juice cocktail.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20568234},
year = {2010}
