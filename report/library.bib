keywords = {detection,explosives,ied,imaging,raman,resonance enhanced,rrs,spectroscopy,standoff,vapors},
pages = {76641K--76641K--12},
title = {{Explosives standoff detection using Raman spectroscopy: from bulk towards trace detection}},
