keywords = {Humans,Male,Models, Biological,Prostatic Diseases,Prostatic Diseases: pathology,Prostatic Diseases: physiopathology,Shear Strength,Urethra,Urethra: physiology,Urinary Bladder,Urinary Bladder: physiology},
month = {jan},
number = {1},
pages = {13--23},
pmid = {17151416},
title = {{Development of a CFD urethral model to study flow-generated vortices under different conditions of prostatic obstruction.}},
