issn = {1936-086X},
journal = {ACS nano},
keywords = {adhesion,afm,bacterial pili,bute to human health,force spectroscopy,including competitive ex-,mechanisms,nanomechanics,probiotics,robiotic bacteria are thought,single-molecule manipulation,through several,to contri-},
month = {mar},
