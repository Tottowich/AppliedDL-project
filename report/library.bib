journal = {Biophysical journal},
keywords = {Adenosine Triphosphatases,Adenosine Triphosphatases: metabolism,Biomechanics,Fimbriae, Bacterial,Fimbriae, Bacterial: chemistry,Fimbriae, Bacterial: metabolism,Movement,Neisseria gonorrhoeae,Neisseria gonorrhoeae: cytology,Probability,Thermodynamics,Time Factors},
month = {feb},
