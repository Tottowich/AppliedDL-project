@article{Asimellis2008,
abstract = {Development and application of an in-situ applicable method to provide rapid determination of platinum group metals (platinum, palladium, and rhodium) elemental concentration in automobile catalyst scrap is reported. Application is based on laser-induced breakdown spectroscopy (LIBS). Actual automobile catalyst slurry in powder form was used to develop the application. With a method requiring approximately 1.5 min of examination per sample, calibration curves are presented with linear regression coefficients close to 0.99 and stability better than 3.0{\%}.},
