issn = {1365-2818},
journal = {Journal of microscopy},
keywords = {Algorithms,Imaging, Three-Dimensional,Imaging, Three-Dimensional: methods,Light,Microscopy,Microscopy: instrumentation,Microscopy: methods,Nanoparticles,Nanoparticles: chemistry,Nanoparticles: radiation effects,Scattering, Radiation},
month = {aug},
number = {2},
pages = {172--8},
pmid = {21375530},
title = {{Three-dimensional automated nanoparticle tracking using Mie scattering in an optical microscope.}},
