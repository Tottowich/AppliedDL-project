journal = {Biochemical and Biophysical Research Communications},
keywords = {1064-nm wavelength laser,Cell division,Cell growth,Damage,Escherichia coli,Hyperbolic curve of damage,Isolated cultivation,On-chip single-cell cultivation,Optical tweezers},
number = {3},
pages = {678--684},
