issn = {0031-9155},
keywords = {Animals,Biological,Models,Muscle,Muscle Contraction,Smooth,Smooth: physiology,Ureter,Ureter: physiology,Urinary Bladder,Urinary Bladder: physiology},
month = {jul},
number = {7},
