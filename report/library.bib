issn = {0743-7463},
journal = {Langmuir : the ACS journal of surfaces and colloids},
keywords = {Antigen-Antibody Complex,Antigen-Antibody Complex: chemistry,Antigen-Antibody Reactions,Immunoglobulin Fragments,Immunoglobulin Fragments: chemistry,Microscopy, Atomic Force,Microscopy, Atomic Force: methods,Muramidase,Muramidase: chemistry,Muramidase: immunology,Surface Properties},
month = {jun},
number = {12},
pages = {5517--23},
pmid = {15924483},
title = {{Antigen binding forces of single antilysozyme Fv fragments explored by atomic force microscopy.}},
