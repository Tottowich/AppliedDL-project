pages = {4068--75},
pmid = {17704145},
publisher = {Elsevier},
title = {{Visco-elastic membrane tethers extracted from Escherichia coli by optical tweezers.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2084229{\&}tool=pmcentrez{\&}rendertype=abstract},
