journal = {Nature},
keywords = {Adhesins, Escherichia coli,Adhesins, Escherichia coli: chemistry,Adhesins, Escherichia coli: metabolism,Crystallization,Escherichia coli Proteins,Escherichia coli Proteins: chemistry,Escherichia coli Proteins: metabolism,Fimbriae Proteins,Fimbriae Proteins: chemistry,Fimbriae Proteins: metabolism,Models, Molecular,Protein Binding,Protein Structure, Quaternary},
month = {jun},
