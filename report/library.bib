journal = {Molecular immunology},
keywords = {Affinity Labels,Affinity Labels: chemistry,Animals,Dimerization,Immunoglobulin G,Immunoglobulin G: chemistry,Immunoglobulin G: drug effects,Mice,Microscopy, Atomic Force,Molecular Probes,Molecular Probes: chemistry,Phycoerythrin,Phycoerythrin: chemistry,Phycoerythrin: pharmacology,Polymers,Protein Conformation,Protein Conformation: drug effects},
month = {nov},
number = {12},
