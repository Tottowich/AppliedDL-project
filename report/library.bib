file = {:E$\backslash$:/Mina Dokument/Mendeley/Arce et al. - Nanoscale structural and mechanical properties of nontypeable Haemophilus influenzae biofilms. - 2009.pdf:pdf},
issn = {1098-5530},
journal = {Journal of bacteriology},
keywords = {Biofilms,Elasticity,Fimbriae, Bacterial,Fimbriae, Bacterial: ultrastructure,Haemophilus influenzae,Haemophilus influenzae: chemistry,Haemophilus influenzae: physiology,Haemophilus influenzae: ultrastructure,Lipopolysaccharides,Lipopolysaccharides: chemistry,Microscopy, Atomic Force},
month = {apr},
number = {8},
pages = {2512--20},
pmid = {19218382},
title = {{Nanoscale structural and mechanical properties of nontypeable Haemophilus influenzae biofilms.}},
