doi = {10.1111/j.1365-2958.2004.04226.x},
file = {:E$\backslash$:/Mina Dokument/Mendeley//Thomas et al. - Shear-dependent 'stick-and-roll' adhesion of type 1 fimbriated Escherichia coli. - 2004.pdf:pdf},
