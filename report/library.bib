doi = {10.1016/j.vaccine.2013.03.027},
file = {:E$\backslash$:/Mina Dokument/Mendeley/Holmgren et al. - Development and preclinical evaluation of safety and immunogenicity of an oral ETEC vaccine containing inactivated E..pdf:pdf},
issn = {1873-2518},
