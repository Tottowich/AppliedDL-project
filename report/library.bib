issn = {1932-6203},
journal = {PloS one},
keywords = {Caliciviridae Infections,Caliciviridae Infections: epidemiology,Caliciviridae Infections: etiology,Caliciviridae Infections: mortality,Child, Preschool,Diarrhea,Diarrhea: epidemiology,Diarrhea: etiology,Diarrhea: mortality,Escherichia coli Infections,Escherichia coli Infections: epidemiology,Escherichia coli Infections: etiology,Escherichia coli Infections: mortality,Humans,Infant,Infant, Newborn,Rotavirus Infections,Rotavirus Infections: epidemiology,Rotavirus Infections: etiology,Rotavirus Infections: mortality},
